@{
    string[] sweets = ViewBag.sweets;
    List<string> fruits = ViewBag.fruits;

}

<h1>Passing Data Demo, Ex02</h1>
<h2>List of Sweets</h2>
<ol>
    @foreach(string sweet in sweets)
            {
                <li>@sweet</li>
            }
</ol>
<br />

<h2>List of Fruits</h2>

<ul>
    @foreach(string fruit in fruits)
            {
                <li>@fruit</li>
            }
</ul>
<br />

<a asp-controller="PassingDataDemo" asp-action="Index">Back to Index</a>